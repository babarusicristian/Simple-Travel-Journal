<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >

    <!-- when is pressed-->
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="32dip" />
            <stroke android:width="1dip" android:color="#132327" />
            <gradient android:angle="-90" android:startColor="#eff7f9" android:endColor="#2f5862"  />
        </shape>
    </item>



    <!-- natural state -->
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="32dip" />
            <stroke android:width="1dip" android:color="#396a75" />
            <gradient android:angle="-90" android:startColor="#eff7f9" android:endColor="#4c8d9c" />
        </shape>
    </item>
</selector>